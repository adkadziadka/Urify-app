<h1>All songs repository:</h1>

<div class="artists-container">
  <% @songs.each do |song| %>
   
	<ul class="songs">
		
		<h3><%= link_to song.title, artist_songs_path(song) %>, by <%= song.artist.name %></h3>
			
  </ul>
   <% end %>
</div>

<footer>
	<%= link_to "All Artists", artists_path %>
  <%= link_to "Add new song", new_artist_song_path %>
</footer>


