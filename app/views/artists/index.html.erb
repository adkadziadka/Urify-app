<h1> Our best artists</h1>

<div class="artist-container">
<% @artists.each do |artist| %>
    <div class="artist"><h3><%= link_to artist.name, artist_path(artist) %></h3>
        <%= image_tag artist.img_url %></div>
<% end %>
</div>
<br>
 


<br>
<footer>
    <%= link_to "Create New Artist", artist_new_artist_path %>
</footer>


