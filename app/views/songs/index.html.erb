<h1>All repository:</h1>

<div class="artists-container">
  <% @songs.each do |song| %>
   
	<ul class="songs">
		
 		<h3><%= link_to song.title, artist_songs_path(song) %></h3>
			
  </ul>
   <% end %>
</div>

<footer>
	<%= link_to "All Artists", artist_url(@songs) %>
